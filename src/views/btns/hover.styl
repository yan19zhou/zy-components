@import 'reset.styl'
body
    color #333
    padding 40px 0 0 0
    font-family "微软雅黑"
    background-color #fff
    margin 0
    display flex
    justify-content space-around
    flex-wrap warp
a
    text-decoration none
    color #37474f
.github-corner:hover .octo-arm
    animation:octocat-wave 560ms ease-in-out

@keyframes octocat-wave
    0%,100%
        transform:rotate(0)
    20%,60%
        transform:rotate(-25deg)
    40%,80%
        transform:rotate(10deg)

.warp
    width 1000px
    margin 0 auto
    padding 0 20px

a[class^="hvr_"]
    display inline-block
    vertical-align middle
    padding 10px 20px
    margin .4em
    cursor pointer
    border-radius 5px
    border 1px solid #DDD
    transform translateZ(0)
    box-shadow 0 0 1px rgba(0, 0, 0, 0)
    backface-visibility hidden //隐藏被旋转的元素的背面
    background-color #f45e61

// 2D Transitions
.hvr_grow
    transition-duration .3s  //定义过渡效果花费的时间
    transition-property transform //规定应用过渡的 CSS 属性的名称
    &:hover
        transform scale(1.1) //放大 
    
.hvr_shrink
    transition-duration .3s  
    transition-property transform 
    &:hover
        transform scale(0.9) //缩小

.hvr_pulse:hover
    animation-name hvr_pulse
    animation-duration 1s
    animation-iteration-count infinite //动画被播放的次数, infinite 动画应该无限次播放
@keyframes hvr_pulse
    25%  
        transform scale(1.1)
    75%  
        transform scale(0.9)
    
.hvr_pulse_grow:hover
    animation-name hvr_pulse_grow
    animation-duration .3s
    animation-iteration-count infinite
    animation-direction alternate  //是否在下一周期逆向地播放, alternate 动画应该轮流反向播放。
@keyframes hvr_pulse_grow
    to
        transform scale(1.1)    
  
.hvr_pulse_shrink:hover
    animation-name hvr_pulse_shrink
    animation-duration .3s
    animation-timing-function linear //动画的速度曲线, linear动画从头到尾的速度是相同的。
    animation-direction alternate 
    animation-iteration-count infinite
@keyframes hvr_pulse_shrink
    to
        transform scale(0.9)

.hvr_push:hover
    animation-name hvr_push
    animation-duration .3s
    animation-timing-function linear
    animation-iteration-count 1 //动画播放次数的数值
@keyframes hvr_push
    50%
        transform scale(0.8) 
    100%
        transform scale(1)   
 
 .hvr_pop:hover
     animation-name hvr_pop
     animation-duration .3s
     animation-timing-function linear
     animation-iteration-count 1
@keyframes hvr_pop
    50%
        transform scale(1.2) 

.hvr_bounce_in
    transform translateZ(0)
    transition-duration 0.5s
    &:hover
        transform scale(1.2)
        transition-timing-function cubic-bezier(0.47, 2.02, 0.31, -0.36)//在 cubic-bezier 函数中自己的值。可能的值是从 0 到 1 的数值
    
.hvr_bounce_out
    transform translateZ(0)
    transition-duration .5s
    &:hover
        transform scale(0.8)
        transition-timing-function cubic-bezier(0.47, 2.02, 0.31, -0.36)

.hvr_rotate
    transform translateZ(0)
    transition-duration .3s
    transition-property transform
    &:hover
        transform rotate(4deg) 
    
.hvr_grow_rotate
    transform translateZ(0)
    transition-duration .3s
    transition-property transform
    &:hover
        transform scale(1.1) rotate(4deg) 
    
.hvr_float
    transform translateZ(0)
    transition-property transform
    transition-duration .3s
    transition-timing-function ease-out //以慢速结束的过渡效果
    &:hover
        transform translateY(-8px)

.hvr_sink
    transition-duration .3s
    transition-timing-function ease-out
    transition-property transform
    &:hover
        transform translateY(8px)

.hvr_bob:hover
    animation-name hvr_bob_float,hvr_bob
    animation-duration .3s, 1.5s
    animation-delay 0s, .3s //规定动画何时开始。默认是 0
    animation-timing-function ease-out, ease-in-out
    animation-iteration-count 1, infinite
    animation-fill-mode forwards //规定对象动画时间之外的状态。forwards当动画完成后，保持最后一个属性值
    animation-direction normal, alternate
@keyframes hvr_bob
    0%
        transform translateY(-8px)
    50%
        transform translateY(-4px)
    100%
        transform translateY(-8px)
@keyframes hvr_float_bob
    100%
        transform translateY(-8px)
    
.hvr_hang:hover
    animation-name hvr_hang_sink,hvr_hang
    animation-duration .3s, 1.5s
    animation-delay 0s, .3s
    animation-timing-function ease-out, ease-in-out
    animation-iteration-count 1, infinite
    animation-fill-mode forwards
    animation-direction normal, alternate
@keyframes hvr_hang
    0%
        transform translateY(8px)
    50%
        transform translateY(4px)
    100%
        transform translateY(8px)
@keyframes hvr_hang_sink
    100%
        transform translateY(8px)    

.hvr_skew
    transition-duration .3s
    transition-property transform
    &:hover
        transform skew(-10deg) //定义沿着 X 和 Y 轴的 2D 倾斜转换。
 
.hvr_skew_forward
    transition-duration .3s
    transition-property transform
    transform-origin 0 100% //允许你改变被转换元素的位置
    &:hover
        transform skew(-10deg)

.hvr_skew_backward
    transition-duration .3s
    transition-property transform
    transform-origin 0 100% //允许你改变被转换元素的位置
    &:hover
        transform skew(10deg)
    
.hvr_wobble_horizontal:hover
    animation-name hvr_wobble_horizontal
    animation-duration 1s
    animation-timing-function ease-in-out
    animation-iteration-count 1
@keyframes hvr_wobble_horizontal
    16.65%
        transform translateX(8px)
    33.3%
        transform translateX(-6px)
    49.95%
        transform translateX(4px)
    66.6%
        transform translateX(-2px)
    83.25%
        transform translateX(1px)
    100%
        transform translateX(0px)

.hvr_wobble_vertical:hover
    animation-name hvr_wobble_vertical
    animation-timing-function ease-in-out
    animation-duration 1s
    animation-iteration-count 1
@keyframes hvr_wobble_vertical
    16.65%
        transform translateY(8px)
    33.3%
        transform translateY(-6px)
    49.95%
        transform translateY(4px)
    66.6%
        transform translateY(-2px)
    83.25%
        transform translateY(1px)
    100%
        transform translateY(0px)
        
.hvr_wobble_to_bottom_right:hover
    animation-name hvr_wobble_to_bottom_right
    animation-duration 1s
    animation-timing-function ease-in-out
    animation-iteration-count 1
@keyframes hvr_wobble_to_bottom_right
    16.65%
        transform translate(8px, 8px)
    33.3%
        transform translate(-6px, -6px)
    49.95%
        transform translate(4px, 4px)
    66.6%
        transform translate(-2px, 2px)
    83.25%
        transform translate(1px, 1px)
    100%
        transform translate(0px, 0px)

.hvr_wobble_to_top_right:hover
    animation-name hvr_wobble_to_top_right
    animation-duration 1s
    animation-timing-function ease-in-out
    animation-iteration-count 1
@keyframes hvr_wobble_to_top_right
    16.65%
        transform translate(8px, -8px)
    33.3%
        transform translate(-6px, 6px)
    49.95%
        transform translate(4px, -4px)
    66.6%
        transform translate(-2px, 2px)
    83.25%
        transform translate(1px, -1px)
    100%
        transform translate(0px, 0px)

.hvr_wobble_top
    transform-origin 0 100%
    &:hover
        animation-name hvr_wobble_top
        animation-duration 1s
        animation-timing-function ease-in-out
        animation-iteration-count 1
@keyframes hvr_wobble_top
    16.65%
        transform skew(-12deg)
    33.3%
        transform skew(10deg)
    49.95%
        transform skew(-6deg)
    66.6%
        transform skew(4deg)
    83.25%
        transform skew(-2deg)
    100%
        transform skew(0)

.hvr_wobble_bottom
    transform-origin 100% 0
    &:hover
        animation-name hvr_wobble_bottom
        animation-duration 1s
        animation-timing-function ease-in-out
        animation-iteration-count 1
@keyframes hvr_wobble_bottom
    16.65%
        transform skew(-12deg)
    33.3%
        transform skew(10deg)
    49.95%
        transform skew(-6deg)
    66.6%
        transform skew(4deg)
    83.25%
        transform skew(-2deg)
    100%
        transform skew(0)
        
.hvr_wobble_skew:hover
    animation-name hvr_wobble_skew
    animation-duration 1s
    animation-timing-function ease-in-out
    animation-iteration-count 1
@keyframes hvr_wobble_skew
    16.65%
        transform skew(-12deg)
    33.3%
        transform skew(10deg)
    49.95%
        transform skew(-6deg)
    66.6%
        transform skew(4deg)
    83.25%
        transform skew(-2deg)
    100%
        transform skew(0)
    
.hvr_buzz:hover
    animation-name hvr_buzz
    animation-duration 0.15s
    animation-timing-function linear
    animation-iteration-count infinite
@keyframes hvr_buzz
    50%
        transform translateX(3px) rotate(2deg)
    100%
        transform translateX(-3px) rotate(-2deg)

.hvr_buzz_out:hover
    animation-name hvr_buzz_out
    animation-duration 0.75s
    animation-timing-function linear
    animation-iteration-count 1
@keyframes hvr_buzz_out
    10%
        transform translateX(3px) rotate(2deg)
    20%
        transform translateX(-3px) rotate(-2deg)
    30%
        transform translateX(3px) rotate(2deg)
    40%
        transform translateX(-3px) rotate(-2deg)
    50%
        transform translateX(2px) rotate(1deg)
    60%
        transform translateX(-2px) rotate(-1deg)
    70%
        transform translateX(2px) rotate(1deg)
    80%
        transform translateX(-2px) rotate(-1deg)
    90%
        transform translateX(1px) rotate(0)
    100%
        transform translateX(-1px) rotate(0)
        


