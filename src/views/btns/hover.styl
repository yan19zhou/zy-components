    &:before
        content ""
        position absolute
        border 6px solid #f45e61
        top 0
        left 0
        right 0
        bottom 0
        animation-duration 1s
    &:hover:before
        animation-name hvr_border_ripple_out
@keyframes hvr_border_ripple_out
    100%
        top -12px 
        right -12px
        bottom -12px
        left -12px
        opacity 1
     
.hvr_border_ripple_in
