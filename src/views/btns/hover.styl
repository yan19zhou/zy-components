        top 0
        left 0
        right 0
        bottom 0
        background-color #0275d8
        transform scaleX(0)
        transform-origin 0 50%
        transition-property transform
        transition-duration .3s
        transition-timing-function  ease-out
    &:hover:before
        transform scaleX(1)  

